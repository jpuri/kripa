<div style="width: 100%">
	<div>
	  <span class="heading">Prices</span>
	  <span style="float: right"><a onclick="Price.newEntity()" class="button_link">Add Price</a>
	  <img id="spinner_new" style="display:none" src="/images/spinner.gif"/></span>
	</div>
	<div style="padding-top: 3%">
	
		<div style="padding-top: 2%" align="left">
		  <div style="width: 22%;float: left;">
		  	  <div style="float: left; width:30%"><%= label_tag(:make, "Make") %></div>
		  	  <div style="float: right; width:70%"><%= collection_select(:make, :id,  @makes, :id, :name, {:prompt => 'Select Make', 
		  	  	:selected => @make_id})%></div>
		 </div>
		 <div style="width: 22%;float: left;padding-left: 5%">
	  		  <div style="float: left; width:30%"><%= label_tag(:model, "Model#", :class => 'medium_lbl_grey') %></div>
			  <div style="float: right; width:70%"><%= collection_select(:model, :id,  @models, :id, :number, {:prompt => 'Select Model', 
			  	:selected => @model_id})%></div>
		 </div>
		 <div style="width: 22%;float: left;padding-left: 5%">
			  <div style="float: left; width:30%"><%= label_tag(:part, "Part#", :class => 'medium_lbl_grey') %></div>
			  <div style="float: right; width:70%"><%= collection_select(:part, :id,  @parts, :id, :number, {:prompt => 'Select Part', 
			  	:selected => @part_id})%></div>
		</div>
		</div>
		<div style="padding-top: 5%" align="left">
		  <div style="width: 22%;float: left;">
			  <div style="float: left; width:30%"><%= label_tag(:customer, "Customer", :class => 'medium_lbl_grey') %></div>
			  <div style="float: right; width:70%"><%= collection_select(:customer, :id,  @customers, :id, :name, 
			  {:prompt => 'Select Customer', :selected => @customer_id})%></div>
			</div>
		 <div style="width: 22%;float: left;padding-left: 5%">
			  <div style="float: left; width:30%"><%= label_tag(:currency, "Currency", :class => 'medium_lbl_grey') %></div>
			  <div style="float: right; width:70%"><%= collection_select(:currency, :id,  @currencies, :id, :name, 
			  {:prompt => 'Select Currency', :selected => @currency_id})%></div>
		</div>
		 <div style="width: 22%;float: left;padding-left: 10%">
			  <a onclick="Price.searchEntity()" class="button_link">Search</a>
			  <img id="spinner_search" style="display:none" src="/images/spinner.gif"/>
			  <a onclick="Price.resetSearchFields()" class="button_link">Reset</a>
			</div>
		</div>
	</div>
	<div id="results" name="results" style="padding-top: 2%; width: 100%">
	  <%= render 'result' %>
	</div>
	<div name="dialog_create" id="dialog_create" class="popup" title="<font color='#60AFFE'>Add Price</font>"></div>
	<div name="dialog_update" id="dialog_update" class="popup" title="<font color='#60AFFE'>Edit Price</font>"></div>
<div>
