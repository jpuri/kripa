<div align="center">
  <form id="updateForm">
  <%= render 'form' %>
  <div style="padding-top: 100px;padding-left: 120px;">
	<a onclick="Part.updateEntity(<%= @part.id%>)" class="button_link"><font color="#EFFBFE">Save</a>
	<img id="spinner_update" style="display:none;" src="/images/spinner.gif"/>
	<a onclick="Part.closeDialog('dialog_update');" class="button_link"><font color="#EFFBFE">Cancel</a></div>
  </div>
  <input type="hidden" value="<%= @part.id%>" id="id" name="id">
</form>
</div>
