<h1>Listing bill_parts</h1>

<table>
  <tr>
    <th>Model price</th>
    <th>Bill</th>
    <th>Quantity</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @bill_parts.each do |bill_part| %>
  <tr>
    <td><%= bill_part.model_price %></td>
    <td><%= bill_part.bill %></td>
    <td><%= bill_part.quantity %></td>
    <td><%= link_to 'Show', bill_part %></td>
    <td><%= link_to 'Edit', edit_bill_part_path(bill_part) %></td>
    <td><%= link_to 'Destroy', bill_part, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Bill part', new_bill_part_path %>
